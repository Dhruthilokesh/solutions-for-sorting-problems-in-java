import java.util.* ;
import java.io.*; 
public class Solution {
	public static void selectionSort(int arr[], int n) {
		for(int i = 0; i< n - 1 ; i++){
			int minVal = i ; 
			for(int j = i +1 ; j <n ; j++)
				if(arr[j] < arr[minVal])
			          minVal = j;
			
			
			 int temp = arr[minVal];
			 arr[minVal] = arr[i];
			 arr[i] =temp; 

		}

		
		}
	
}
